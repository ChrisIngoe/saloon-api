{
  "rules": {
      "indent": [
          2,
          2
      ],
      "quotes": [
          2,
          "single"
      ],
      "semi": [
          2,
          "always"
      ]
  },
  "env": {
      "node": true,
      "es6": true,
      "mocha": true
  },
  "extends": ["eslint:recommended", "plugin:security/recommended"],
  "plugins": [
      "security"
  ],
  "parserOptions": {
      "ecmaVersion": 2018
  }
}